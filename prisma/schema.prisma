// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model TransactionEvent {
  hash String @id
  timeStamp     Int
  gasPrice      Int
  gasUsed       Int
  transactionFeeInUSDT Float
}